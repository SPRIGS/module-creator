{
  "name": "sprigs/module-creator",
  "type": "project",
  "description": "A laravel command for module creation",
  "keywords": [
    "module",
    "creator"
  ],
  "homepage": "https://github.com/SPRIGS/module-creator",
  "license": "MIT",
  "authors": [
    {
      "name": "Kreshnik Hasanaj",
      "email": "kreshnik@sprigs.eu",
      "homepage": "https://www.sprigs.eu",
      "role": "CTO"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/console": "^5.8.15|^6.0|^7.0|^8.0",
    "illuminate/contracts": "^5.8.15|^6.0|^7.0|^8.0",
    "illuminate/filesystem": "^5.8.15|^6.0|^7.0|^8.0",
    "illuminate/support": "^5.8.15|^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "SPRIGS\\Module\\Creator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SPRIGS\\Module\\Creator\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "SPRIGS\\Module\\Creator\\ModuleCreatorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}